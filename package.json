  "homepage": "",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "src/index.ts",
  "module": "dist/index.m.js",
  "scripts": {
    "preinstall": "node -e \"!process.env.npm_config_user_agent.startsWith('pnpm/') && !console.log('Use \\`npx pnpm install\\` to install dependencies in this repository\\n') && process.exit(1)\"",
    "run:ts": "ts-node -r tsconfig-paths/register src/index.ts",
    "test:ts": "tsc -p tsconfig.json --noEmit --watch",
    "test:ts:verbose": "tsc -p tsconfig.json --traceResolution --noEmit",
    "upload": "clasp push",
    "clasp:test": "npm run test:ts && npm run upload && clasp run buildAddOn",
    "deploy": "clasp:test && clasp run buildAddOn; terminal-notifier -title \"sparsewx\" -message \"done\"",
    "lint:ts": "tslint src/index.ts src/Code/**/*.ts",
    "getty": "x=$(echo {}); echo $x | jq"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paxperscientiam/sparse-wx.git"
  },
  "keywords": [
    "weather",
    "apps-script"
  ],
  "dependencies": {
    "@paxperscientiam/dlv.ts": "^1.2.5",
    "auto-bind": "^4.0.0",
    "autobind-decorator": "^2.4.0",
    "bind-decorator": "^1.0.11",
    "core-js": "^3.6.4",
    "dset": "^2.0.1",
    "es6-set": "^0.1.5",
    "idx": "^2.5.6",
    "object-path": "^0.11.4",
    "query-string": "^6.11.0",
    "ramda": "^0.26.1",
    "stately.js": "^2.0.0",
    "tsconfig-paths": "^3.9.0",
    "uri-js": "^4.2.2",
    "urijs": "^1.19.2"
  },
  "devDependencies": {
    "@types/gapi": "0.0.36",
    "@types/gapi.client.drive": "^3.0.4",
    "@types/gapi.client.gmail": "^1.0.1",
    "@types/gapi.client.groupssettings": "^1.0.2",
    "@types/google-apps-script": "0.0.37",
    "@types/node": "^11.15.6",
    "@types/object-path": "^0.11.0",
    "fuse-box": "^4.0.0-next.165",
    "ts-node": "^8.6.2",
    "tslib": "^1.10.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5"
  }
}
