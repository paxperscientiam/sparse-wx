{
  "compilerOptions": {
    "types" : ["app", "nws", "node", "google-apps-script", "@paxperscientiam/dlv.ts"],
    "strict": true,
    "typeRoots": [
      "src/@types",
      "node_modules/@types"
    ],
    "outDir": "dist",
    "baseUrl": "src",
    "paths": {
      "App/*": ["./*"]
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "module": "commonjs",
    "target": "es6"
  },
  "exclude": [
    "src/Code/Tests.ts"
  ],
  "include": [
  ],
  "files": [
    "src/index.ts"
  ]
}
